<h3 style='text-align:center;'> Team Allocation Results </h3>

<div class='container'>
	<div class='row'>
		<div class='col-md-6 col-md-offset-3'>
			<table>
				{{#each teams}}
					<tr>
					<td> Team {{@index}}</td>
					{{#each this}}
						<td>&nbsp; (Member {{@index}} - {{this}}) &nbsp;</td>
					{{/each}}
					</tr>
				{{/each}}
			</table>
		<div>
	</div>
</div>
